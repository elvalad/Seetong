<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/n_bg_dd" />
            <solid android:color="@color/n_title_bg_dd" />
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/n_bg_dd" />
            <solid android:color="@color/n_title_bg" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</selector>